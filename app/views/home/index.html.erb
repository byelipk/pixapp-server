<h1>Home#index</h1>
<p>Find me in app/views/home/index.html.erb</p>

<%= form_for @picture, url: pictures_path, html: { multipart: true } do |form| %>
  <%= form.file_field :image %>
  <%= form.submit("Upload") %>
<% end %>


<ul>
<% @pictures.each do |p| %>
  <li><%= image_tag p.image.url(:medium) %></li>
<% end %>
</ul>
